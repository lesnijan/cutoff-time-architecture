# Application Configuration
APP_NAME="Cutoff Time API"
VERSION="1.0.0"
ENVIRONMENT=development
DEBUG=true
LOG_LEVEL=DEBUG

# API Configuration
API_V1_PREFIX=/api/v1
ALLOWED_ORIGINS=["http://localhost:3000","http://localhost:8080"]

# SAP HANA Configuration
HANA_HOST=localhost
HANA_PORT=30015
HANA_USER=SYSTEM
HANA_PASSWORD=YourStrongPassword123
HANA_DATABASE=SYSTEMDB
HANA_ENCRYPT=true
HANA_POOL_SIZE=10

# Redis Configuration
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=
REDIS_DB=0
REDIS_TTL=60

# Rate Limiting
RATE_LIMIT_ENABLED=true
RATE_LIMIT_CAPACITY_CHECK=100
RATE_LIMIT_CUTOFF=300
RATE_LIMIT_STATUS=60
RATE_LIMIT_SIMULATE=10

# Business Logic Configuration
MAX_UTILIZATION=0.85
SAFETY_BUFFER_MINUTES=30
VIP_RESERVE_PERCENT=0.10
CONGESTION_ALPHA=1.2

# Monitoring
METRICS_ENABLED=true
METRICS_PATH=/metrics

# Security
JWT_SECRET_KEY=changeme-in-production-use-strong-random-key
JWT_ALGORITHM=HS256
JWT_EXPIRATION_MINUTES=60
